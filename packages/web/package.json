{
  "name": "@splitshare/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf node_modules",
    "prestart": "yarn gen",
    "start": "react-scripts start",
    "start:nb": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen": "graphql-codegen --config codegen.yml",
    "svgr": "npx @svgr/cli --config-file .svgrrc.js --ext tsx icons --out-dir src/components/icons"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.39",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@auth0/auth0-spa-js": "^1.6.4",
    "@reach/router": "^1.3.3",
    "@types/graphql": "^14.5.0",
    "animejs": "^3.1.0",
    "clsx": "^1.1.0",
    "fast-deep-equal": "^3.1.1",
    "graphql": "^14.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-gravatar": "^2.6.3",
    "react-scripts": "3.4.0",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.0.1",
    "try-to-catch": "^3.0.0",
    "use-query-params": "^0.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@svgr/cli": "^5.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/animejs": "^3.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/reach__router": "^1.3.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-gravatar": "^2.6.7",
    "@types/react-transition-group": "^4.2.4",
    "@types/styled-components": "^5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts|tsx}",
      "!/node_modules/",
      "!src/**/*.stories.{js|ts|tsx}"
    ]
  }
}
